function y =  Gauss( n )

syms x;

for j=1:1:(2*n-1)
    phi(j) = 1 / j;
end
for j=1:1:n
    for k=1:1:n
        A(j,k) = phi(n+j-k);
    end
end

for j=1:1:n
    b(j) = -phi(n+j - 1);
end

a = A\(b');

temp = x^n;

for j=1:1:n
    temp = temp + x^(n-j) * a(j);
end

disp(vpa(temp));


roots = solve(temp);

for j=1:1:n
    coef(j) = int(temp /(roots(j) * diff(temp)),0,1);
end

disp(vpa(y));


end

